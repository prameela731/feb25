# Online Python - IDE, Editor, Compiler, Interprete
l= []
t = []
n= int(input("Enter n:"))
for i in range(1, n + 1):
    x = int(input())
    l.append(x)

for i in range(n):
    if l[i]>0:
       t.append(l[i])
       x+=1
for i in range(n):
    if l[i]<0:
        t.append(l[i])
print(t)

